{
    "name": "paygo-dev/paygo-sdk-php",
    "description": "Cliente em PHP para integração com as APIs de consulta de transações",
    "type": "libraries",
    "support": {
        "issues": "https://github.com/paygo-dev/paygo-sdk-php/issues",
        "source": "https://github.com/paygo-dev/paygo-sdk-php"
    },
    "authors": [
        {
            "name": "Adriano M. La Selva",
            "email": "a.moreira@paygo.com.br"
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "5.3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-0": {
            "": "src/"
        },
        "psr-4": {
            "PayGo\\": "src/PayGo/",
            "Tests\\PayGo\\": "tests/PayGo/"
        }
    }
}
